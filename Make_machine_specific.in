#
# MINC machine-specific Makefile
#

# Make macro for using target in dependency (gmake is different from other
# makes)
TARGET_IN_DEPENDENCY = @TARGET_IN_DEPENDENCY@

# Commands and their options
OPT           = -O
MACHINE_LDOPT = @LIBS@
CC            = @CC@
RM            = rm
RM_FLAGS      = -f
AR            = ar
AR_FLAGS      = -r
RANLIB        = @RANLIB@
SHELL         = /bin/sh
CP            = cp
CP_FLAGS      = 
CHMOD         = chmod
CHMOD_XFLAGS  = 755
CHMOD_FLAGS   = 644
NROFF         = @NROFF@
NROFF_FLAGS   = @NROFF_FLAGS@
CD            = cd
LINT          = lint
LINT_LIBOPTS  = -m -x -u
LINT_PROGOPTS = -m -x -u

# OS type for fortran stuff
OS_TYPE       = @OS_TYPE@

# Suffix for lint
.SUFFIXES: .ln


